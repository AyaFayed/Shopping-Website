<%- include ("partials/header") %>
  <body>
    <%- include ("partials/navbar") %>
    <% if (results.length==0) { %>
      <div class="m-4" style="color: aliceblue"><h3 class="mx-auto" style="width: 300px;">Item Not Found</h3></div>
    <% }%> <% for (let result of results) { %>
      <div class="m-1">
    <a
      class="card-light"
      href="<%=`${result.ref}`%>"
      style="text-decoration: none"
    >
      <div class="card-horizontal">
        <div class="img-square-wrapper">
          <img class="" src="<%= result.image %>" width="100" height="130" />
        </div>
        <div class="card-body">
          <h4 class="card-title res"><%= result.name %></h4>
          <h6 class="card-subtitle mb-2 text-muted"><%= result.category %></h6>
          <h6 class="card-subtitle mb-3 mt-2">Price : <%= result.price %> $</h6>
        </div>
      </div>
    </a>
  </div>
    <% } %>
  </body>
</html>
